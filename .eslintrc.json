{
	"env": {
		"browser": false,
		"commonjs": false,
		"es2021": false,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"@typescript-eslint/no-var-requires": "off",
		"no-mixed-spaces-and-tabs": "warn"
	}
}
